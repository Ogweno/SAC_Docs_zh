{
    "plugins": [
        "include-codeblock",
        "katex",
        "editlink",
        "anchors",
        "page-toc",
        "-sharing",
        "forkmegithub",
        "image-captions",
        "git-author"
    ],
    "pluginsConfig": {
        "editlink": {
            "base": "https://github.com/seisman/SAC_Docs_zh/edit/gitbook/",
            "label": "编辑此页面",
            "multilingual": false
        },
        "forkmegithub": {
            "url": "https://github.com/seisman/SAC_Docs_zh"
        },
        "image-captions": {
            "caption": "图 _BOOK_IMAGE_NUMBER_ : _CAPTION_"
        },
        "git-author":{
            "position": "bottom",
            "createTpl": "Created by {user} {timeStamp}",
            "modifyTpl": "Last modified by {user} {timeStamp}",
            "timeStampFormat": "YYYY-MM-DD HH:mm"
        }
    }
}
